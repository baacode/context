{
    "name": "erayd/context",
    "description": "Extract content from web documents",
    "homepage": "https://github.com/erayd/context",
    "type": "library",
    "license": "ISC",
    "require": {
        "php": "^7.0.0",
        "ext-mbstring": "^7.0",
        "ext-dom": "20031129"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "authors": [{
        "name": "Steve Gilberd",
        "email": "steve@erayd.net"
    }],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Context\\": "src/"
        }
    },
    "scripts": {
        "style-check": "phpcs --standard=PSR2 src",
        "style-fix": "phpcbf --standard=PSR2 src"
    }
}
